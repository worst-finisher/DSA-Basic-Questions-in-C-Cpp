#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    if(n%2==0 && n%3==0)
    {
        cout<<"n is divisible by both"<<endl;
    }
    else if(n%2==0)
    {
        cout<<"n is divisible by 2 only"<<endl;
    }
    else if(n%3==0)
    {
      cout<<"n is divisible by 3 only"<<endl;
    }
    else
    {
        cout<<"value is not correct"<<endl;
    }

    return 0;
}
