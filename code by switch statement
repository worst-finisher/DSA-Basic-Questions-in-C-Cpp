#include <iostream>
using namespace std;
int main()
{
    char button,a,b,c,d;
    cin>>button;
    switch(button)
    {
        case 'a':
        cout<<"Hello";
        break;
        case 'b':
        cout<<"Namaste";
        break;
        case 'c':
        cout<<"Kem Chho";
        break;
        case 'd':
        cout<<"Ki Haal Bhau Ke";
        break;
        default:
        cout<<"wrong button";
        break;
    }
    return 0;
}
