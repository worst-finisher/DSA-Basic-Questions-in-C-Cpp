#include<iostream>
using namespace std;
int main()
{
    int n;       // n belongs to any integer value
    cin>>n; 
    if(n>10)    //we can also change 10 by any number from which we have to compare the other numbers
    {
        cout<<"n is greater than 10"<<endl;
    }
    else if(n<10)
    {
        cout<<"n is smaller than 10"<<endl;
    }
    else
    {
        cout<<"n is equal to 10"<<endl;
    }

    return 0;
}
