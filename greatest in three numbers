#include<iostream>
using namespace std;
int main()
{
    int a,b,c;
    cin>>a>>b>>c;
    if(a>b)
    {
        if(a>c)
        {
            cout<<"a is greater";
        }
        else
        {
            cout<<"c greater";
        }
    }
    else if(b>a)
    {
        if(b>c)
        {
            cout<<"b greater";
        }
        else
        {
            cout<<"c greater";
        }
    }
    else
    {
        cout<<"c is greater";
    }
    return 0; 
}
