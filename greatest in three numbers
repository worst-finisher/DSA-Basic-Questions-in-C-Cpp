#include<iostream>
using namespace  std;
int main()
{
    int a,b,c;
    cin>>a>>b>>c;
    if(a>b)
    {
        if(a>c)
        {
            cout<<"a greater = "<<a;
        }
        else
        {
            cout<<"c greater = "<<c;
        }
    }
    else if(b>a)
    {
        if(b>c)
        {
            cout<<"b greater = "<<b;
        }
        else
        {
            cout<<"c greater = "<<c;
        }
    }
    else
    {
        cout<<"c greater = "<<c;
    }
    
    return 0;
}
